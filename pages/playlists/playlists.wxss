/* pages/playlists/playlists.wxss */
page {
  background-color: #FFF;
}
.container {
  margin: 20rpx 0;
  padding: 10rpx 15rpx;
  border: none;
  background-color: #FFF;
}
.container .title {
  font-size: 35rpx;
  font-weight: 700;
  margin: 15rpx 0;
}
.container .list-box {
  display: flex;
  flex-flow: wrap row;
  justify-content: flex-start;
  align-items: center;
}
.container .list-box .list-item {
  padding: 10rpx;
}
.more {
  display: flex;
  flex-flow: nowrap column;
  justify-content: center;
  align-items: center;
  margin: 20rpx 0;
}
