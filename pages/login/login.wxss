/* pages/login/login.wxss */
page {
  background-color: #DD001B;
}
.container {
  display: flex;
  flex-direction: column;
  padding: 10rpx 35rpx;
  height: 100vh;
}
.container .logo {
  flex: 2;
  margin: auto;
}
.container .logo .logo-img {
  width: 200rpx;
  height: 200rpx;
  border-radius: 30rpx;
}
.container .login {
  flex: 1;
  padding: 0 40rpx;
  font-weight: 700;
}
.container .van-button {
  color: #DD001B;
}
.container .van-center-enter-to {
  width: 90%;
}
